    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "^9",
    "prettier": "^3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "supabase": "^2"
  },
  "resolutions": {
    "@types/react": "^19"
  },
  "overrides": {
    "@types/react": "^19"
  }
