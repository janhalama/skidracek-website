    "supabase:reset": "supabase db reset",
    "supabase:link": "bash scripts/supabase-link.sh",
    "supabase:push": "supabase db push",
    "supabase:deploy": "supabase db push",
    "supabase:migration-list": "supabase migration list",
    "supabase:migration-new": "supabase migration new",
    "supabase:migration-up": "supabase migration up",
    "supabase:shell": "supabase db shell",
    "supabase:query": "supabase db shell --command",
    "supabase:generate-types": "supabase gen types typescript > lib/types/supabase.ts",
    "supabase:ensure": "bash scripts/supabase-ensure.sh",
