{
  "name": "temp-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run supabase:ensure && npm run supabase:setup-env && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "postbuild": "bash scripts/migrate-prod.sh",
